<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.chance.learn_gridview">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/zjft"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity
            android:name=".Welcome"
            android:label="紫金ATM">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainBusiness"
            android:label="业务办理">
        </activity>
        <activity
            android:name=".business.ProcDrawCash"
            android:label="快捷取款" />
        <activity
            android:name=".business.ProcInquire"
            android:label="账户查询" />
        <activity
            android:name=".business.ProcDoTransfer"
            android:label="转账汇款" />
        <activity
            android:name=".business.ProcOther"
            android:label="其它业务" />

    </application>

</manifest>